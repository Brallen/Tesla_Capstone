{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "sass --no-source-map Assets/Styles/Sass/main.sass Assets/Styles/main.css && node ./bin/www",
    "client": "cd login && yarn start",
    "server": "nodemon app.js",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\""
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "hbs": "^4.0.2",
    "http-errors": "~1.6.2",
    "morgan": "~1.9.0",
    "sass": "^1.17.0",
    "teslajs": "^4.2.6"
  },
  "devDependencies": {
    "concurrently": "^4.0.1"
  }
}
